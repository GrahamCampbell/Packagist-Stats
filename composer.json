{
    "name": "graham-campbell/packagist-stats",
    "description": "Packagist Stats Is A CLI Tool To Display Download Stats For Packagist Packages",
    "type": "project",
    "keywords": ["packagist", "stats", "packages", "cl", "PackagistStats", "Packagist Stats", "Packagist-Stats", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk"
        }
    ],
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "composer/xdebug-handler": "^1.4 || ^2.0 || ^3.0",
        "guzzlehttp/guzzle": "^7.4.3",
        "guzzlehttp/psr7": "^2.2.1",
        "symfony/console": "^4.3.8 || ^5.0 || ^6.0",
        "symfony/error-handler": "^4.4 || ^5.0 || ^6.0",
        "knplabs/packagist-api": "^1.7.2"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0.7",
        "phpunit/phpunit": "^8.5.25"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\PackagistStats\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\PackagistStats\\": "tests/"
        }
    },
    "bin": [
        "bin/stats"
    ],
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": "dist"
    },
    "extra": {
        "bin-dir": "bin"
    }
}
